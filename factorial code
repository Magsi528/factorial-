#include <iostream>

using namespace std;


unsigned long long factorial(int n) {
    if (n < 0) {
        cout << "Please enter a positive number" << endl;
        return 0;
    }

    unsigned long long result = 1;
    for (int i = 1; i <= n; ++i) {
        result *= i;
    }

    return result;
}

int main() {
    int n;
    cout << "Enter a number: ";
    cin >> n;

    unsigned long long result = factorial(n);
    if (n >= 0) {
        cout << "Factorial of " << n << " is: " << result << endl;
    }

    return 0;
}
